			"name": "Python Magic Variable",
			"scope": "python.support.magic.variable",
			"settings": {
				"foreground": "#FB467B"
			}
		},
		{
			"name": "Function",
			"scope": "meta.function-call, variable.function, support.function, keyword.other.special-method, meta.block-level",
