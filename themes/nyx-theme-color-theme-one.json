	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#5c6370",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#96c474",
				"fontStyle": ""
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
				"variable.other.constant"
			],
			"settings": {
				"foreground": "#d19a66",
				"fontStyle": ""
			}
		},
		{
			"name": "User-defined constant & Language variable",
			"scope": [
				"constant.character",
				"variable.language"
			],
			"settings": {
				"foreground": "#c776df",
				"fontStyle": ""
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable.readwrite",
				"variable.readwrite.other.block",
				"support.type.object.module"
			],
			"settings": {
				"foreground": "#e16971",
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"keyword.operator.new",
				"keyword.operator.expression",
				"punctuation.definition.keyword",
				"storage"
			],
			"settings": {
				"foreground": "#c776df",
				"fontStyle": ""
			}
		},
		{
			"name": "Separator",
			"scope": [
				"punctuation.separator.key-value",
				"punctuation.destructuring",
				"punctuation.separator.annotation.python",
				"punctuation.separator.dict.python"
			],
			"settings": {
				"foreground": "#56b6c2",
				"fontStyle": ""
			}
		},
		{
			"name": "Class name",
			"scope": [
				"entity.name.class",
				"entity.name.module",
				"entity.name.type",
				"entity.other.inherited-class",
				"storage.identifier",
				"support.class",
				"support.other.namespace",
				"support.type"
			],
			"settings": {
				"foreground": "#eedd92",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Function call",
			"scope": [
				"meta.function-call",
				"meta.method-call",
				"support.function.construct.output",
				"entity.name.function.stylus",
				"support.function.filter.css",
				"support.function.misc"
			],
			"settings": {
				"foreground": "#56b6c2",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Function definition",
			"scope": [
				"meta.definition.function",
				"meta.definition.method",
				"entity.name.function.stylus",
				"support.function.filter.css"
			],
			"settings": {
				"foreground": "#5eadf2",
				"fontStyle": ""
			}
		},
		{
			"name": "Punctuation & Variable",
			"scope": [
				"punctuation.accessor",
				"punctuation.definition.arguments",
				"punctuation.separator.delimiter",
				"punctuation.terminator",
				"punctuation.section",
				"constant.name.attribute.tag",
				"punctuation.definition.begin",
				"punctuation.definition.end",
				"punctuation.definition.parameters.begin",
				"punctuation.definition.parameters.end",
				// Variable
				"variable.other",
				"support.variable",
				"meta.function-call.arguments",
				// Library constant
				"support.constant"
			],
			"settings": {
				"foreground": "#abb2c1",
				"fontStyle": ""
			}
		},
		{
			"name": "Builtin Functions",
			"scope": [
				"function.support.builtin",
				"function.support.core"
			],
			"settings": {
				"foreground": "#96c474",
				"fontStyle": ""
			}
		},
		{
			"name": "Template expression",
			"scope": "meta.template.expression",
			"settings": {
				"foreground": "#eedd92",
				"fontStyle": ""
			}
		},
		{
			"name": "Tag name",
			"scope": [
				"entity.name.tag",
				"entity.name.tag.class.js",
				"entity.name.tag.class.jsx"
			],
			"settings": {
				"foreground": "#e16971",
				"fontStyle": ""
			}
		},
		{
			"name": "Tag Class and ID",
			"scope": [
				"entity.name.tag.class",
				"entity.name.tag.id",
				"entity.other.attribute-name.id"
			],
			"settings": {
				"foreground": "#56b6c2",
				"fontStyle": ""
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#d19a66",
				"fontStyle": ""
			}
		},
		{
			"name": "Attribute Class",
			"scope": "entity.other.attribute-name.class",
			"settings": {
				"foreground": "#eedd92",
				"fontStyle": ""
			}
		},
		{
			"name": "StyleSheet Property Name",
			"scope": [
				"support.type.property-name.css",
				"support.type.property-name.postcss",
				"support.type.property-name.scss",
				"support.type.property-name.less",
				"support.type.property-name.sass"
			],
			"settings": {
				"foreground": "#abb2bf",
				"fontStyle": ""
			}
		},
		{
			"name": "StyleSheet Property value",
			"scope": [
				"support.constant.css",
				"support.constant.postcss",
				"support.constant.scss",
				"support.constant.less",
				"support.constant.sass"
			],
			"settings": {
				"foreground": "#96c474",
				"fontStyle": ""
			}
		},
		{
			"name": "StyleSheet Variable",
			"scope": [
				"variable.css",
				"variable.postcss",
				"variable.scss",
				"variable.less",
				"variable.sass"
			],
			"settings": {
				"foreground": "#abb2c1",
				"fontStyle": ""
			}
		},
		{
			"name": "StyleSheet Variable String",
			"scope": [
				"variable.css.string",
				"variable.postcss.string",
				"variable.scss.string",
				"variable.less.string",
				"variable.sass.string"
			],
			"settings": {
				"foreground": "#e5c07b",
				"fontStyle": ""
			}
		},
		{
			"name": "StyleSheet Unit",
			"scope": [
				"unit.css",
				"unit.postcss",
				"unit.scss",
				"unit.less",
				"unit.sass"
			],
			"settings": {
				"foreground": "#c776df",
				"fontStyle": ""
			}
		},
		{
			"name": "StyleSheet Function",
			"scope": [
				"function.css",
				"function.postcss",
				"function.scss",
				"function.less",
				"function.sass"
			],
			"settings": {
				"foreground": "#56b6c2",
				"fontStyle": ""
			}
		},
		{
			"name": "JSON Property Name",
			"scope": [
				"support.type.property-name",
				"meta.object-literal.key",
				"variable.object.property"
			],
			"settings": {
				"foreground": "#e16971",
				"fontStyle": ""
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#e16971",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Link",
			"scope": [
				"string.detected-link",
				"linkForeground"
			],
			"settings": {
				"foreground": "#eedd92",
				"fontStyle": " italic underline"
			}
		},
		{
			"name": "Diff header",
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#5c6370",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Diff deleted",
