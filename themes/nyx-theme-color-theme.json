	"tokenColors": [
		{
			"name": "Comments",
			"scope": "comment, punctuation.definition.comment",
			"settings": {
				"foreground": "#5C6370",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Python Docstring",
			"scope": "string.quoted.docstring.multi.python, string.quoted.docstring.multi.python keyword.control.flow.python",
			"settings": {
				"foreground": "#5C6370",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Brackets",
			"scope": "meta.brace.round.begin, meta.brace.round.end, meta.brace.square.begin, meta.brace.square.end, meta.brace.curly.begin, meta.brace.curly.end, meta.group.braces.round.function.arguments",
			"settings": {
				"foreground": "#97a7c8ff"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Boolean",
			"scope": "Boolean",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#CB6CFE"
			}
		},
		{
			"name": "Regex String",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "String URLs",
			"scope": "string.detected-link",
			"settings": {
				"foreground": "#CB6CFE"
			}
		},
		{
			"name": "Numbers and Built-in Constants",
