			"name": "Object",
			"scope": "variable.other.object",
			"settings": {
				"foreground": "#FB467B"
			}
		},
		{
			"name": "Object Property",
			"scope": "meta.property.object",
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"name": "Classes",
			"scope": "variable.other.class, meta.class.property variable.other.property",
			"settings": {
				"foreground": "#FB467B"
			}
